        {
            "when": {
                "west": true
            },
            "apply": {
                "model": "esperia:robust_glass_pane_silver_side_alt",
                "y": 90
            }
        },
        {
            "when": {
                "north": false
            },
            "apply": {
                "model": "esperia:robust_glass_pane_silver_noside"
            }
        },
        {
            "when": {
                "east": false
            },
            "apply": {
                "model": "esperia:robust_glass_pane_silver_noside_alt"
            }
        },
        {
            "when": {
                "south": false
            },
            "apply": {
                "model": "esperia:robust_glass_pane_silver_noside_alt",
                "y": 90
            }
